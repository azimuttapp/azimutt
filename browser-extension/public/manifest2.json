{
  "manifest_version": 2,
  "name": "Azimutt",
  "version": "0.3",
  "homepage_url": "https://azimutt.app",
  "description": "Next-Gen ERD: Design, Explore, Document and Analyze your database.",
  "icons": {
    "16": "azimutt_16.png",
    "32": "azimutt_32.png",
    "48": "azimutt_48.png",
    "128": "azimutt_128.png"
  },
  "permissions": [],
  "content_scripts": [
    {
      "matches": [
        "https://github.com/*"
      ],
      "js": [
        "appendButton.js"
      ]
    }
  ],
  "background": {
    "scripts": [
      "background.js"
    ]
  },
  "browser_action": {
    "default_icon": {
      "16": "azimutt_16.png",
      "32": "azimutt_32.png",
      "48": "azimutt_48.png",
      "128": "azimutt_128.png"
    },
    "default_title": "Open Azimutt"
  }
}
